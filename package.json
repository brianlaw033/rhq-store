{
  "name": "rhq-store",
  "version": "1.0.1",
  "description": "RemoteHQ Store",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianlaw033/rhq-store.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "typescript",
    "npm",
    "template",
    "ts"
  ],
  "author": "Geoinsight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianlaw033/rhq-store/issues"
  },
  "homepage": "https://github.com/brianlaw033/rhq-store#readme",
  "dependencies": {
    "@react-pdf/types": "^2.3.3",
    "aws-amplify": "^5.3.11",
    "memoizee": "^0.4.15",
    "moment": "^2.29.4",
    "semantic-ui-react": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.13.0",
    "@types/react": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
